<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.wear.basket.mapper.CerBoardMapper">
  	<select id = "selectcerboard" resultType = "com.wear.basket.vo.CerBoardVO">
  		select * from cancel_echange_return
  		where order_no = #{orderNo}
  	</select>
  	
  	<select id = "cerboardList" resultType = "com.wear.basket.vo.CerBoardVO" parameterType = "int">
  		select * from cancel_echange_return
  		where cancel_echange_return_no = #{cancel_echange_returnNo}
  	</select>
  </mapper>