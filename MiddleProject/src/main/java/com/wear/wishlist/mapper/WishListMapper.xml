<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wear.wishlist.mapper.WishListMapper">
  <select id = "selectwishlist" resultType = "com.wear.wishlist.vo.WishListVO">
  	select *
  	from tbl_wishlist
  </select>
  
  <delete id = "deletewishList" parameterType = "int">
  	delete from tbl_wishlist
  	where wishlist_no = #{wishlistNo}
  </delete>
</mapper>