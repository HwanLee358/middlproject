<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wear.review.mapper.ProductMapper">

	<select id="productList" parameterType="int" resultType="product">
		select * from product
		where product_no = #{productNo}
	</select>


</mapper>